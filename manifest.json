{
    "name": "Browsick",
    "version": "0.3",
    "description": "Browse With Music - This Chrome extension lets you play lo-fi in the background while surfing the web.",
    "browser_action": {
        "default_title": "Browsick",
        "default_popup": "popout.html"
    },
    "background": {
        "page": "background.html",
        "persistent": true
    },
    "icons": { "16": "assets/icons/16.png",
        "48": "assets/icons/48.png",
       "128": "assets/icons/128.png" },
    "manifest_version": 2
}